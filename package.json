		"gulp",
		"js",
		"sass",
		"pug",
		"linters",
		"webpack"
	],
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead",
		"not IE 11"
	],
	"author": "Oleksandr Pereiaslov",
	"license": "WTFPL",
	"devDependencies": {
