{
	"name": "my-gulp-pug",
	"version": "1.0.0",
	"description": "Gulp-Pug",
	"scripts": {
		"dev": "gulp watch",
		"build:dev": "gulp build",
		"build:prod": "gulp build --prod",
		"build": "webpack --config webpack.config.js"
	},
